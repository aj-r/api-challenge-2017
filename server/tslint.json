{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "arrow-parens": [true, "ban-single-arg-parens"],
        "curly": false,
        "max-line-length": [true, 180],
        "member-ordering": false,
        "no-console": false,
        "no-angle-bracket-type-assertion": true,
        "no-empty": false,
        "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}
